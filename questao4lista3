peso = float(input("Digite o peso: "))
alt = float(input("Digite a altura: "))
imc = (peso/alt**2)
if(imc<18.5):
  print("abaixo do peso")
elif(imc>=18.5 and imc<=24.9):
  print("peso ideal")
elif(imc>25 and imc<= 29.9):
  print("sobrepeso")
elif(imc>30 and imc<=34.9):
  print("obesidade grau 1")
else:
  print("obesidade grau 3")
