num1 = float(input("Digite o primeiro numero: "))
num2 = float(input("Digite o segundo numero: "))
num3 = float(input("Digite o terceiro numero: "))
menor = 0

if(num1<num2 and num1<num3):
    menor = num1
    print(menor)
elif(num2<num1 and num2<num3):
    menor = num2
    print(menor)
else:
    print(num3)
